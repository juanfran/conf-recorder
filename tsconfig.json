{
	"compilerOptions": {
    "types": ["node"],
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2022",
    "lib": ["ESNext.Array", "esnext"]
	},
	"include": ["src"]
}
